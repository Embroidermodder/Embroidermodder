                     "../project-files/debian/data/usr/share/mime-info/embroidermodder2.mime"

debpixmaps.path   = "/usr/share/pixmaps"
debpixmaps.files  = "../project-files/debian/data/usr/share/pixmaps/embroidermodder2.png"

INSTALLS += embapp      \
            embtips     \
            embcommands \
            embhelp     \
            embicons    \
            embimages   \
            embsamples  \
            embtrans    \
            debusrbin       \
            deblibmime      \
            debappregistry  \
            debapplications \
            debdoc          \
            debman          \
            debmenu         \
            debmimexml      \
            debmimeinfo     \
            debpixmaps      \

}

