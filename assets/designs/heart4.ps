% Postscript functions for the heart4 shape.
% This file is part of Embroidermodder 2.

/range {0.0 2.0} def
/num_points 512 def
/scale {0.04 0.04} def

/heart4x {
    /t exch def
    (*
        (cos t)
        ((sin(t)*sqrt(abs(cos(t))))/(sin(t)+7/5) - 2*sin(t) + 2)
    )
}

/heart4y {
    /t exch def
    (*
        (sin t)
        ((sin(t)*sqrt(abs(cos(t))))/(sin(t)+7/5) - 2*sin(t) + 2)
    )
}

